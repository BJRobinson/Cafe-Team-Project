package team_project;
public class waiter {
  String name;
  int id;
  String password;

  public waiter(String name, int id, String password) {
    this.name = name;
    this.id = id;
    this.password = password;
  }

  public String getPassword() {
    return this.password;
  }

  public void setPassword(String password) {
    this.password = password;
  }

  public int getID() {
    return this.id;
  }

  public void setID(int id) {
    this.id = id;
  }


  public void setName(String name) {
    this.name = name;
  }

  public  String getName() {
    return this.name;
  }
  
  //update order shaould take an int as a parameter to update DB
  
  public void confirmOrder(Order order)
  {order.updateOrder(2);}
  

  public void orderEnRoute(Order order)
  {order.updateOrder(4);}
  
  public void orderArrived(Order order)
  {order.updateOrder(5);}
}


  


